{
  "name": "haste",
  "version": "0.1.0",
  "private": true,
  "description": "Private Pastebin Server",
  "keywords": [
    "paste",
    "pastebin"
  ],
  "author": {
    "name": "John Crepezzi",
    "email": "john.crepezzi@gmail.com",
    "url": "http://seejohncode.com/"
  },
  "main": "haste",
  "dependencies": {
    "busboy": "0.2.4",
    "connect": "3.4.1",
    "connect-ensure-login": "^0.1.1",
    "connect-query": "^0.2.0",
    "connect-ratelimit": "0.0.7",
    "connect-redirection": "0.0.1",
    "connect-route": "0.1.5",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "nodemon": "^1.11.0",
    "pg": "4.1.1",
    "redis": "0.8.1",
    "redis-url": "0.1.0",
    "st": "1.1.0",
    "uglify-js": "1.3.3",
    "winston": "0.6.2"
  },
  "devDependencies": {
    "mocha": "*",
    "passport": "^0.3.2",
    "passport-google-oauth20": "^1.0.0",
    "should": "*"
  },
  "bundledDependencies": [],
  "engines": {
    "node": "0.10.35",
    "npm": "1.1.49"
  },
  "bin": {
    "haste-server": "./server.js"
  },
  "files": [
    "server.js",
    "lib",
    "static"
